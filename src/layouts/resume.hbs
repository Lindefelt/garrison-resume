---js
{
  document: {
    title: "Konsultprofil",
    propertiesHeading: "Kompetens",
    projectsHeading: "Uppdrag (urval)",
    projectsSections: {
      descriptionHeading: "Uppdrag",
      roleHeading: "Roll",
      sectorHeading: "Bransch",
      methodHeading: "Metod",
      resultHeading: "Kundnytta"
    }
  },
  permalink: '/{{ profile.name | slug }}/resume/'
}
---
<!doctype html>
<html class="no-js" lang="{{site.lang}}">
  <head>
    <meta charset="utf-8">
    <title>{{#if profile.name}}{{slug profile.name}}{{/if}}{{#if profile.title}}-{{slug profile.title}}{{/if}}-garrison</title>
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link rel="stylesheet" media="screen, print" href="/resume.css">
  </head>
  <body class="Page">
    <table class="Page-wrapper">
      <thead><tr><td>
        <div class="Page-header">
          <header class="Header">
            <div class="Grid">
              <div class="Grid-item--start4 Grid-item--span8">
                <ul>
                  <li>{{#if title}}{{title}}{{else}}{{document.title}}{{/if}}</li>
                  <li>{{profile.email}}</li>
                  <li>{{profile.phone}}</li>
                </ul>
              </div>
            </div>
          </header>
        </div>
      </td></tr></thead>
      <tbody><tr><td>

        <main class="Page-main">
          <div class="Head">
            <div class="Grid">
              <div class="Grid-item--start4 Grid-item--span8">
                <h1>{{profile.name}}</h1>
                <h2>{{profile.title}}</h2>
              </div>
            </div>
          </div>

          <section class="Profile Section">
            <div class="Grid">
              <div class="Grid-item--span4">
                {{#if profile.picture}}<img class="Image" src="{{profile.picture}}">{{/if}}
              </div>
              <div class="Grid-item--span8">
                {{#if content}}
                  {{{content}}}
                {{else if profile.summary}}
                  {{{profile.summary}}}
                {{/if}}
              </div>
            </div>
          </section>

          <section class="Lists Section">
            <header class="Section-header">
              <div class="Grid">
                <div class="Grid-item--start4 Grid-item--span8">
                  <h3>{{#if properties.title}}{{properties.title}}{{else}}{{document.propertiesHeading}}{{/if}}</h3>
                </div>
              </div>
            </header>

            {{#if properties.skills}}
            {{#unless properties.skills.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.skills.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <p>
                    {{#each properties.skills.items}}{{#if @first}}{{this}}{{else}}, {{this}}{{/if}}{{/each}}
                  </p>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.sectors}}
            {{#unless properties.sectors.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.sectors.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <p>
                  {{#each properties.sectors.items}}{{#if @first}}{{this}}{{else}}, {{this}}{{/if}}{{/each}}
                  </p>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.clients}}
            {{#unless properties.clients.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.clients.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <p>
                    {{#each properties.clients.items}}{{#if @first}}{{this}}{{else}}, {{this}}{{/if}}{{/each}}
                  </p>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.methods}}
            {{#unless properties.methods.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.methods.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <p>
                    {{#each properties.methods.items}}{{#if @first}}{{this}}{{else}}, {{this}}{{/if}}{{/each}}
                  </p>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if tools}}
            {{#unless properties.tools.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.tools.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <ul>
                  {{#each properties.tools.items}}
                    <li>{{this}}</li>
                  {{/each}}
                  </ul>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.programmingLanguages}}
            {{#unless properties.programmingLanguages.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.programmingLanguages.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <p>
                    {{#each properties.programmingLanguages.items}}{{#if @first}}{{this}}{{else}}, {{this}}{{/if}}{{/each}}
                  </p>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.education}}
            {{#unless properties.education.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.education.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <ul>
                  {{#each properties.education.items}}
                    <li>{{area}}{{#if institution}}, {{institution}}{{#if startDate}}, {{dateFormat startDate "YYYY"}}{{/if}}{{#if endDate}} – {{dateFormat endDate "YYYY"}}{{/if}}{{/if}}</li>
                  {{/each}}
                  </ul>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.courses}}
            {{#unless properties.courses.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.courses.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <ul>
                  {{#each properties.courses.items}}
                    <li>{{area}}{{#if institution}}, {{institution}}{{#if startDate}}, {{dateFormat startDate "YYYY"}}{{/if}}{{#if endDate}} – {{dateFormat endDate "YYYY"}}{{/if}}{{/if}}</li>
                  {{/each}}
                  </ul>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.employment}}
            {{#unless properties.employment.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.employment.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <ul>
                  {{#each properties.employment.items}}
                    <li>{{company}}{{#if startDate}}, {{dateFormat startDate "YYYY"}}{{/if}}{{#if @first}} - pågående{{else}}{{#if endDate}} – {{dateFormat endDate "YYYY"}}{{/if}}{{/if}}</li>
                  {{/each}}
                  </ul>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}

            {{#if properties.languages}}
            {{#unless properties.languages.hidden}}
            <div class="List">
              <div class="Grid">
                <div class="Grid-item--span4">
                  <h4>{{properties.languages.title}}</h4>
                </div>
                <div class="Grid-item--span8">
                  <p>
                  {{#each properties.languages.items}}{{#if @first}}{{language}}{{else}}, {{language}}{{/if}} ({{fluency}}){{/each}}
                </p>
                </div>
              </div>
            </div>
            {{/unless}}
            {{/if}}
          </section>

          {{#if projects}}
          <section class="Projects Section">

            <header class="Section-header">
              <div class="Grid">
                <div class="Grid-item--start4 Grid-item--span8">
                  <h3>{{#if projects.title}}{{projects.title}}{{else}}{{document.projectsHeading}}{{/if}}</h3>
                </div>
              </div>
            </header>

            {{#each projects.items}}
              {{#unless hidden}}
              <div class="Project">
                <div class="Grid">
                  <div class="Grid-item Grid-item--span4">
                    <header class="Project-header">
                      <h4>{{client}}</h4>
                      <p>{{#if project}}{{project}}, {{/if}}{{#if startDate}}{{startDate}}{{/if}}{{#if endDate}}–{{endDate}}{{/if}}</p>
                      {{#if types}}
                      <div><p>{{#each types}}{{#if @first}}{{this}}{{else}}, {{this}}{{/if}}{{/each}}</p></div>
                      {{/if}}
                    </header>
                  </div>
                  <div class="Grid-item Grid-item--span8">
                    {{#if role}}
                    <div class="Project-section">
                      <h4>{{#if @root.projects.sections.role}}{{@root.projects.sections.role}}{{else}}{{@root.sections.projects.sections.role}}{{/if}}</h4>
                      {{#if @root.projects.raw}}
                        {{{role}}}
                      {{else}}
                      <p>
                        {{role}}
                      </p>
                      {{/if}}
                    </div>
                    {{/if}}
                    {{#if description}}
                    <div class="Project-section">
                      <h4>{{#if @root.projects.sections.description}}{{@root.projects.sections.description}}{{else}}{{@root.sections.projects.sections.description}}{{/if}}</h4>
                      {{#if @root.projects.raw}}
                        {{{description}}}
                      {{else}}
                      <p>
                        {{description}}
                      </p>
                      {{/if}}
                    </div>
                    {{/if}}
                    {{#if method}}
                    <div class="Project-section">
                      <h4>{{#if @root.projects.sections.method}}{{@root.projects.sections.method}}{{else}}{{@root.sections.projects.sections.method}}{{/if}}</h4>
                      {{#if @root.projects.raw}}
                        {{{method}}}
                      {{else}}
                      <p>
                        {{method}}
                      </p>
                      {{/if}}
                    </div>
                    {{/if}}
                    {{#if result}}
                    <div class="Project-section">
                      <h4>{{#if @root.projects.sections.result}}{{@root.projects.sections.result}}{{else}}{{@root.sections.projects.sections.result}}{{/if}}</h4>
                      {{#if @root.projects.raw}}
                        {{{result}}}
                      {{else}}
                      <p>
                        {{result}}
                      </p>
                      {{/if}}
                    </div>
                    {{/if}}
                    {{#if sections}}
                    {{#each sections}}
                    <div class="Project-section">
                      {{#if title}}<h4>{{title}}</h4>{{/if}}
                      {{#if body}}
                      {{#if @root.projects.raw}}
                        {{{body}}}
                      {{else}}
                      <p>
                        {{body}}
                      </p>
                      {{/if}}
                      {{/if}}
                    </div>
                    {{/each}}
                    {{/if}}
                  </div>
                </div>
              </div>
              {{/unless}}
            {{/each}}
          </section>
          {{/if}}
        </main>

      </td></tr></tbody>
      <tfoot><tr><td>
        <div class="Page-footer">
          <footer class="Footer">
            <div class="Grid">
              <div class="Grid-item--start4 Grid-item--span2">
                <img src="/images/logo-black.svg" class="Logo">
              </div>
            </div>
          </footer>
        </div>
      </td></tr></tfoot>
    </table>
  </body>
</html>
